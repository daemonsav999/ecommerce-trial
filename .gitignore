# Create .gitignore file with appropriate contents
cat > .gitignore << EOL
# dependencies
/node_modules

# expo
/.expo
/.expo-shared

# environment variables
.env

# debug
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# android/ios build
/android/build
/ios/build

# misc
.DS_Store
*.pem

# local env files
.env.local
.env.development.local
.env.test.local
.env.production.local

# typescript
*.tsbuildinfo
EOL