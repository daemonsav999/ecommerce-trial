    "@react-navigation/native": "^7.0.15",
    "@react-navigation/stack": "^7.1.2",
    "@reduxjs/toolkit": "^2.2.1",
    "axios": "^1.8.2",
    "date-fns": "^3.3.1",
    "expo": "~52.0.37",
    "expo-barcode-scanner": "~12.9.2",
    "expo-blur": "~14.0.3",
    "expo-camera": "~16.0.17",
    "expo-constants": "~17.0.7",
    "expo-dev-client": "~5.0.12",
    "expo-file-system": "~18.0.11",
    "expo-font": "~13.0.4",
