    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3"
  },
  "private": true,
  "eslintConfig": {
    "extends": "universe/native"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-fast-image",
          "react-native-maps",
          "react-native-chart-kit",
          "expo-barcode-scanner",
          "redux-persist"
        ]
      }
    }
  }
